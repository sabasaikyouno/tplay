
GET     /                           controllers.HomeController.index
POST    /post_text                  controllers.HomeController.postText
POST    /post_image                 controllers.HomeController.postImage
GET     /getImage/tmp/img/:fileName controllers.HomeController.getImage(fileName)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
